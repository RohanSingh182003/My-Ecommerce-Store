{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "text",
      "required": true
    },
    "price": {
      "type": "integer",
      "required": true
    },
    "discount": {
      "type": "integer"
    },
    "catagory": {
      "type": "enumeration",
      "enum": [
        "fashion",
        "grocerry",
        "electronics"
      ],
      "required": true
    },
    "image": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": false,
      "required": true
    },
    "slug": {
      "type": "uid",
      "required": true
    }
  }
}
